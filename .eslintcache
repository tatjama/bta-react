ted.logInUser.status === 1) \r\n                        && <NavLink to=\"/providers\" activeClassName=\"active\"><li id=\"providers\" to=\"/providers\">Providers</li></NavLink>\r\n                    }\r\n                        </>\r\n                        : <>\r\n                        <NavLink to=\"/signin\" activeClassName=\"active-sign\"><li>Sign in</li></NavLink>\r\n                        <NavLink to=\"/signup\" activeClassName=\"active-sign\"><li>Sign Up</li></NavLink>                        \r\n                        </>\r\n                    }\r\n                                    \r\n                </ul>\r\n            </div>\r\n            <AppContext.Provider value = {{ isUserAuthenticated, setUserHasAuthenticated}} >  \r\n              <Routes/> \r\n              </AppContext.Provider>       \r\n        <Footer/>\r\n        </div>\r\n    )\r\n}\r\n\r\nexport default App;","C:\\projects\\github\\bta-react\\src\\hooks\\useStateWithSessionStorage.js",[],"C:\\projects\\github\\bta-react\\src\\pages\\Routes.js",[],"C:\\projects\\github\\bta-react\\src\\components\\Footer.js",[],"C:\\projects\\github\\bta-react\\src\\libs\\contextLib.js",[],"C:\\projects\\github\\bta-react\\src\\pages\\SignUp.js",[],"C:\\projects\\github\\bta-react\\src\\pages\\SignIn.js",[],"C:\\projects\\github\\bta-react\\src\\pages\\Home.js",[],"C:\\projects\\github\\bta-react\\src\\pages\\Feedbacks.js",[],"C:\\projects\\github\\bta-react\\src\\pages\\Providers.js",[],"C:\\projects\\github\\bta-react\\src\\pages\\City.js",[],"C:\\projects\\github\\bta-react\\src\\hooks\\useForm.js",["199"],"import {useState, useEffect} from 'react';\r\n\r\n const useForm = (callback, validate, initValues) => {     \r\n     \r\n    const [values, setValues] = useState(initValues);\r\n    const [errors, setErrors] = useState({});    \r\n    const [isSent, setIsSent] = useState(false);  \r\n    const [isSubmitting, setIsSubmitting] = useState(false);     \r\n    \r\n      const handleChange = (e)=>{\r\n        const {name, value} = e.target;\r\n        setIsSubmitting(false);\r\n        const newValues = {...values, [name]: value};       \r\n        isSent && setErrors(validate(newValues));\r\n        setValues(values =>(newValues));\r\n      };     \r\n    \r\n      const handleSubmit = (e)=>{\r\n        e.preventDefault();\r\n        setErrors(validate(values)); \r\n        setIsSubmitting(true);\r\n        setIsSent(true);          \r\n       }   \r\n\r\n      useEffect(()=>{ \r\n          if(Object.keys(errors).length === 0 \r\n          && isSubmitting \r\n          && isSent           \r\n           ){\r\n             callback(                      \r\n                setValues(initValues)\r\n              )\r\n          }\r\n      }, [errors, isSent, isSubmitting, initValues])        \r\n\r\n      const handleReset = (e)=>{\r\n        setValues(initValues);\r\n      }\r\n\r\n   return{\r\n    values,\r\n    handleChange,\r\n    handleSubmit,\r\n    handleReset,\r\n    errors,\r\n    isSubmitting\r\n} \r\n}\r\n\r\nexport default useForm;","C:\\projects\\github\\bta-react\\src\\components\\validateSignUp.js",[],"C:\\projects\\github\\bta-react\\src\\components\\validateSignIn.js",[],"C:\\projects\\github\\bta-react\\src\\components\\Button.js",[],"C:\\projects\\github\\bta-react\\src\\components\\ProviderInfo.js",[],"C:\\projects\\github\\bta-react\\src\\components\\CommentFromUsers.js",[],"C:\\projects\\github\\bta-react\\src\\components\\SelectForm.js",[],"C:\\projects\\github\\bta-react\\src\\components\\ProviderDetailForm.js",[],"C:\\projects\\github\\bta-react\\src\\pages\\Airports.js",[],"C:\\projects\\github\\bta-react\\src\\pages\\Attractions.js",[],"C:\\projects\\github\\bta-react\\src\\pages\\CityInfo.js",[],"C:\\projects\\github\\bta-react\\src\\pages\\Restaurants.js",[],"C:\\projects\\github\\bta-react\\src\\pages\\Hotels.js",[],"C:\\projects\\github\\bta-react\\src\\api.js",[],"C:\\projects\\github\\bta-react\\src\\components\\SectionFirst.js"